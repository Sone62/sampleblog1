generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Channels { 
id          Int @id @default(autoincrement()) 
X           Float 
Y           Float 
Z           Int 
Type        String 
Name        String 
Items       String 
Desc        String 
Postcode    Int
Estatecode  Int
Address     String 
Building    String
}