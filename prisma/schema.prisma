generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
// Channels model represents results
model Channels {
  id        Int       @id @default(autoincrement())
  X         Float
  Y         Float
  Z         Int
  Type      String
  Name      String
  Items     String
  Desc      String
  Postcode  Int
  Address   String
}

// Post model represents individual blog posts
model Post {
  id        Int       @id @default(autoincrement())
  title     String
  content   String
}