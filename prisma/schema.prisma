generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Channels {
  id                Int       @id @default(autoincrement())
  X                 Float
  Y                 Float
  Z                 Int
  Type              String
  Name              String
  Items             String
  Desc              String
  Postcode          Int
  Address           String
  Building          String
  INC_CRC           String
  FMEL_UPD_D        Float
}

model Post {
  id      Int    @id @default(autoincrement())
  title   String
  content String
}
