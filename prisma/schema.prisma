generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Channel {
  id         Int     @id @default(autoincrement())
  type       String
  name       String
  items      String
  desc       String
  postcode   Int
  estateCode Int
  address    String
  building   String
  x          Float
  y          Float
}
